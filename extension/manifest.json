{
  "name": "Rabbit Hole - Bookmark & Tab Organizer",
  "description": "Advanced text analysis and visualization tool.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {},
  "icons": {
    "16": "diving bunny neutral.png",
    "48": "diving bunny neutral.png",
    "128": "diving bunny neutral.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "sidePanel",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "settingsInit.js"
  },
  "side_panel": {
    "default_path": "analysisSidepanel.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "tallying.js"
      ]
    }
  ]
}